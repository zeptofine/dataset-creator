{
  "editor.formatOnSave": true,
  "python.analysis.typeCheckingMode": "basic",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true,
    "editor.rulers": [
      120
    ]
  },
  "pylint.args": [
    "--extension-pkg-whitelist=cv2",
    "--good-names=i,j,k,p,s,t,ex,Run,df",
    "--max-line-length=120"
  ],
  "black-formatter.args": [
    "--line-length=120"
  ],
  "cSpell.words": [
    "chunksize",
    "configtypes",
    "dataframe",
    "datarules",
    "dtype",
    "dunders",
    "EXTMATCH",
    "filedb",
    "GLOBSTAR",
    "imdataset",
    "IMREAD",
    "imwrite",
    "Keyworded",
    "lmao",
    "ndarray",
    "pbar",
    "rprint",
    "timeparser",
    "typer",
    "wcmatch",
    "wglob"
  ]
}